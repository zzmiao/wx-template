/* pages/clearrect/clarrect.wxss */
.clearrect-box{
  border: 1rpx solid red;
  width: 400rpx;
  height: 200rpx;
  margin: auto;
  position: relative;
  
}
.clearrect-images{
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin:50rpx auto;
}
.clear-canvas{
  border: 1rpx solid blue;
  width: 400rpx;
  height: 200rpx;
  position: absolute;
  top: -1rpx;
  left: -1rpx;
  z-index: 1
}